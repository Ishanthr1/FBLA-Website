y */\n  .calendar-day:hover {\n    transform: scale(1.05);\n  }\n\n  /* Event container */\n  .event-container {\n    width: 90%;\n    max-height: 60px;\n    overflow-y: auto;\n    background: rgba(255, 255, 255, 0.8);\n    padding: 3px;\n    border-radius: 5px;\n    text-align: center;\n  }\n\n  /* Event label */\n  .event-label {\n    font-weight: bold;\n    font-size: 12px;\n    color: #2E7D32;\n    display: block;\n    margin: 2px 0;\n  }\n\n  /* Highlight event days */\n  .event-day {\n    background-color: #f0fceb; /* Light blue for event days */\n  }\n\n  /* Hover effect for event days */\n  .event-day:hover {\n    transform: scale(1.1);\n    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.15);\n  }\n\n.event-hover-box {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  width: 220px; /* Adjust size if needed */\n  background: rgba(255, 255, 255, 0.95);\n  padding: 30px;\n  border-radius: 10px;\n  box-shadow: 0 6px 15px rgba(0, 0, 0, 0.3);\n  transform: translate(-50%, -50%) scale(0);\n  transition: transform 0.2s ease-in-out;\n  text-align: center;\n  pointer-events: none; /* Prevent interfering with other interactions */\n}\n\n.event-day {\n  position: relative;\n  z-index: 9999;/* Ensure child elements like the hover box stay inside */\n}\n\n.event-day:hover .event-hover-box {\n  transform: translate(-50%, -50%) scale(1);\n  z-index: 9999; /* Ensures it's above all other elements on the page */\n  pointer-events: auto; /* Allow interaction with the hover box */\n}\n\n/* For visual effect: make the event container and other events look more distinct */\n.event-container {\n  width: 90%;\n  max-height: 60px;\n  overflow-y: auto;\n  background: rgba(255, 255, 255, 0.8);\n  padding: 3px;\n  border-radius: 5px;\n  text-align: center;\n}\n\n\n  /* Event content inside hover box */\n  .hover-content h3 {\n    font-size: 14px;\n    margin-bottom: 5px;\n  }\n\n  .hover-content p {\n    font-size: 12px;\n    margin: 3px 0;\n  }\n\n  /* Buy ticket button */\n  .buy-ticket-b